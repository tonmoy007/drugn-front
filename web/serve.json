 {
  "headers": [
    {
      "source": "static/**/*.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
   "rewrites": [
     { "source": "**", "destination": "/index.html" }
   ],
   "cleanUrls": true
}
